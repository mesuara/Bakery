

    <div class="ui three column grid">
    <% @info.each do |item| %>
  <div class="column">
    <div class="ui fluid card">
      <div class="image">
        <img src=<%=item.image%> class="cardimg">
      </div>
      <div class="content">
        <a class="header"><%=item.title%></a>
        <h3>$<%=item.price%></h3>
      </div>
    </div>
  </div>
  <%end%>
  </div>
